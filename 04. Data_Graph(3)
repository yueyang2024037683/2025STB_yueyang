> library(readxl)
> survey <- read_excel("04. 2025STB_survey.xlsx")
> View(survey)

# table()함수/구분 1개의 인자를 가지고 도수분포표 작성
table(survey$Gender)#성별

# table()함수/구분 1개의 인자를 가지고 상대도수분포표를 작성
ECN <- table(survey$Gender)#ECN에 성별의 도수분포표를 대입
prop.table(ECN)#상대 도수로 표시

# table()함수/구분 2개의 인자를 가지고 교차표를 작성
table(survey$Gender, survey$Grade)

#막대그래프1
barplot(table(survey$Nationality),xlab="Nationality", ylab="number", main="survey's nationalities distributions within my class",col="yellow")

# table()함수/구분 1개의 인자를 가지고 (가로)막대그래프를 작성
barplot(table(survey$`Residential area`),xlab="Residential area", ylab="number", main="survey's Residential area distributions within my class",col=terrain.colors(12))#주거지역

#누적-막대그래프
entry <- table(survey$Gender, survey$Grade)
barplot(entry, legend = TRUE)

#파이차트1
pie(table(survey$Grade),main="survey's Grade distributions within my class",col=terrain.colors(12))

#히스토그램 작성1
hist(survey$Age,col=terrain.colors(12))

#기술통계
summary(survey) 
summary(survey, na.rm=T)
                                          
#survey의 Grade 박스플롯 작성1
boxplot(survey$Grade, main="survey's Grade distributions boxplot within my class",  col="blue")

#survey의 Age 박스플롯 작성2
boxplot(survey$Age, main="survey's Age distributions boxplot within my class", col="green")
 
#박스플롯 작성3:병렬 비교 
boxplot(survey$Grade, survey$Age, main="survey's Grade and Age distributions boxplot within my class", col="green", names = c("Grade","Age"))
 
#산점도1
plot(x=survey$Grade, y=survey$Age, xlab="Grade", ylab="Age", main="survey's Grade and Age distributions boxplot within my class")

#산점도2
plot(x=survey$Grade, y=survey$Age, xlab="Grade", ylab="Age", main="survey's Grade and Age distributions boxplot within my class", pch=24, col="red", bg="yellow", cex=1.5)

#산점도3
plot(x=survey$Grade, y=survey$Age, xlab="Grade", ylab="Age", main="survey's Grade and Age distributions boxplot within my class", type = "h")